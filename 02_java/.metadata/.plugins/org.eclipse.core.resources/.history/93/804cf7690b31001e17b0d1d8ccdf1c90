package java_study_chap_05;

public class ArrayEx9 {

	static int findmax(int[] data) {
		int max = save[0];
		for (int i = 0; i < save.length; i++) {
			if (max < data[i]) {
				max = data[i];
			}
		}
		return max;

	}

	static int findMin(int[] data) {
		int min = save[0];
		for (int i = 0; i < save.leng; i++) {
			if (min > data[i]) {
				min = data[i];
			}
		}
		return min;
	}

