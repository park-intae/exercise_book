package java_study_chap_03;

public class input2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n1 = 10;
		int n2 = 3;

		System.out.println(n1 + n2);
		System.out.println(n1 - n2);
		System.out.println(n1 * n2);
		System.out.println(n1 / n2); // 정수/정수=정수
		System.out.println(n1 / 4.0); // 장수/실수 || 실수/정수 = 실수

		String str1 = "안녕하세요";
		String str2 = "안녕하세요";

		if (str1 == str2) {
			System.out.println("두 문자열 일치");
		}
	}

}
