package java_study_chap_05;

public class Method {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		printCharacter('*', 30);
		System.out.println("Hellow, Java");
		printCharacter('-', 20);
	}

	static void printCharacter(char ch, int num) {
		for (int cnt = 0; cnt <= num; cnt++) {
			System.out.print(ch);
		}
		System.out.println("");
	}

}
